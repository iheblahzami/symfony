{% extends 'base.html.twig' %}

{% block title %}Incomes{% endblock %}

{% block body %}
    <h1>Incomes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ income.id }}</td>
            </tr>
            <tr>
                <th>Montant</th>
